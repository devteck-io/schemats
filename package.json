{
  "name": "schemats-v2",
  "version": "3.0.4",
  "description": "Generate typescript interface definitions from (postgres) SQL database schema",
  "keywords": [
    "postgres",
    "schema",
    "typescript",
    "sql"
  ],
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dependency-check": "dependency-check . --entry bin/schemats.js --missing --no-dev",
    "test": "npm run build && npm run dependency-check && mocha",
    "prepublish": "npm run build",
    "clean": "del-cli node_modules **/*.js",
    "coverage": "npm run lint && npm run build && npm run dependency-check && nyc mocha",
    "coverage:ci": "npm run lint && npm run build && npm run dependency-check && nyc mocha && nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "schemats": "bin/schemats.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sweetiq/schemats.git"
  },
  "bugs": {
    "url": "https://github.com/sweetiq/schemats/issues"
  },
  "author": "Mengxuan Xia <xiamx2004@gmail.com>",
  "contributors": [
    "Arnaud Benhamdine <arnaud.benhamdine@gmail.com>",
    "zigomir <zigomir@gmail.com>",
    "Mark Crisp <macr1324@gmail.com>",
    "Xonlly Devteck <contact@devteck.eu>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/diff": "^4.0.2",
    "@types/lodash": "^4.14.37",
    "@types/mocha": "^7.0.2",
    "@types/mysql": "^2.15.9",
    "@types/mz": "^2.7.0",
    "@types/node": "^13.9.0",
    "@types/power-assert": "^1.4.29",
    "@types/proxyquire": "^1.3.27",
    "@types/sinon": "^7.5.2",
    "@types/yargs": "^15.0.4",
    "coveralls": "^3.0.9",
    "del-cli": "^3.0.0",
    "dependency-check": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "power-assert": "^1.4.2",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "diff": "^4.0.2",
    "lodash": "^4.17.4",
    "mysql": "^2.13.0",
    "mz": "^2.6.0",
    "pg-promise": "^10.4.4",
    "typescript": "^3.8.3",
    "typescript-formatter": "^7.2.2",
    "yargs": "^15.3.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
